<protounitsounddefmods>
  <protounit mergeMode="replace" name="deREVCetbang">
    <soundtype name="Select">
      <soundset name="IndonesiaMilitarySelect">
      </soundset>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="GenericMaleGrunt">
      </soundset>
    </soundtype>
    <soundtype name="Death">
      <soundset name="GenericMaleDeath">
      </soundset>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="MilitaryBirth">
      </soundset>
    </soundtype>
    <soundtype name="Acknowledge">
      <targetlogic>
        <choice name="default">
          <soundset name="IndonesiaMilitaryAcknowledge">
          </soundset>
        </choice>
        <choice name="enemy">
          <soundset name="IndonesiaMilitaryAttack">
          </soundset>
        </choice>
      </targetlogic>
    </soundtype>
  </protounit>
</protounitsounddefmods>