<protounitsounddefmods>
  <protounit name="Settler">
    <soundtype name="Select">
      <variationlogic>
        <choice name="0">
          <civlogic>
            <choice mergeMode="add" name="LBDanish">
              <soundset name="DanishSettlerMaleSelect">
              </soundset>
            </choice>
          </civlogic>
        </choice>
        <choice name="1">
          <civlogic>
            <choice mergeMode="add" name="LBDanish">
              <soundset name="DanishSettlerFemaleSelect">
              </soundset>
            </choice>
          </civlogic>
        </choice>
      </variationlogic>
    </soundtype>
    <soundtype name="Grunt">
      <variationlogic>
        <choice name="0">
          <soundset name="GenericMaleGrunt">
          </soundset>
        </choice>
        <choice name="1">
          <soundset name="GenericFemaleGrunt">
          </soundset>
        </choice>
      </variationlogic>
    </soundtype>
    <soundtype name="Death">
      <variationlogic>
        <choice name="0">
          <soundset name="GenericMaleDeath">
          </soundset>
        </choice>
        <choice name="1">
          <soundset name="GenericFemaleDeath">
          </soundset>
        </choice>
      </variationlogic>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="SettlerBirth">
      </soundset>
    </soundtype>
    <soundtype name="Acknowledge">
      <variationlogic>
        <choice name="0">
          <civlogic>
            <choice mergeMode="add" name="LBDanish">
              <targetlogic>
                <choice name="default">
                  <soundset name="DanishSettlerMaleAcknowledge">
                  </soundset>
                </choice>
                <choice name="enemy">
                  <soundset name="DanishSettlerMaleAttack">
                  </soundset>
                </choice>
                <choice name="build">
                  <soundset name="DanishSettlerMaleBuild">
                  </soundset>
                </choice>
                <choice name="Huntable">
                  <soundset name="DanishSettlerMaleGatherMeat">
                  </soundset>
                </choice>
                <choice name="Tree">
                  <soundset name="DanishSettlerMaleGatherWood">
                  </soundset>
                </choice>
                <choice name="AbstractFarm">
                  <soundset name="DanishSettlerMaleFarm">
                  </soundset>
                </choice>
                <choice name="AbstractMine">
                  <soundset name="DanishSettlerMaleGatherCoin">
                  </soundset>
                </choice>
                <choice name="AbstractResourceCrate">
                  <soundset name="DanishSettlerMaleAcknowledge">
                  </soundset>
                </choice>
                <choice name="AbstractFruit">
                  <soundset name="DanishSettlerMaleGatherFruit">
                  </soundset>
                </choice>
              </targetlogic>
            </choice>
          </civlogic>
        </choice>
        <choice name="1">
          <civlogic>
            <choice mergeMode="add" name="LBDanish">
              <targetlogic>
                <choice name="default">
                  <soundset name="DanishSettlerFemaleAcknowledge">
                  </soundset>
                </choice>
                <choice name="enemy">
                  <soundset name="DanishSettlerFemaleAttack">
                  </soundset>
                </choice>
                <choice name="build">
                  <soundset name="DanishSettlerFemaleBuild">
                  </soundset>
                </choice>
                <choice name="Huntable">
                  <soundset name="DanishSettlerFemaleGatherMeat">
                  </soundset>
                </choice>
                <choice name="Tree">
                  <soundset name="DanishSettlerFemaleGatherWood">
                  </soundset>
                </choice>
                <choice name="AbstractFarm">
                  <soundset name="DanishSettlerFemaleFarm">
                  </soundset>
                </choice>
                <choice name="AbstractMine">
                  <soundset name="DanishSettlerFemaleGatherCoin">
                  </soundset>
                </choice>
                <choice name="AbstractResourceCrate">
                  <soundset name="DanishSettlerFemaleAcknowledge">
                  </soundset>
                </choice>
                <choice name="AbstractFruit">
                  <soundset name="DanishSettlerFemaleGatherFruit">
                  </soundset>
                </choice>
              </targetlogic>
            </choice>
          </civlogic>
        </choice>
      </variationlogic>
    </soundtype>
  </protounit>
</protounitsounddefmods>