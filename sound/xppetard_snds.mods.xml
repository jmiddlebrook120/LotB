      <civlogic>
        <choice name="Spanish">
          <soundset name="DanishMortarSelect">
          </soundset>
        </choice>
      </civlogic>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="GenericMaleGrunt">
      </soundset>
    </soundtype>
    <soundtype name="Death">
      <soundset name="GenericMaleDeath">
      </soundset>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="MilitaryBirth">
      </soundset>
    </soundtype>
    <soundtype name="Acknowledge">
      <civlogic>
        <choice name="DEMaltese">
          <targetlogic>
            <choice name="default">
              <soundset name="DanishMortarAcknowledge">
              </soundset>
            </choice>
            <choice name="enemy">
              <soundset name="DanishMortarAttack">
              </soundset>
            </choice>
          </targetlogic>
        </choice>
      </civlogic>
