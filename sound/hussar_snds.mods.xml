<protounitsounddefmods>
  <protounit name="Hussar">
    <soundtype name="Select">
      <civlogic>
        <choice mergeMode="add" name="LBDanish">
          <soundset name="DanishHussarSelect">
          </soundset>
        </choice>
      </civlogic>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="CavalryGrunt">
      </soundset>
    </soundtype>
    <soundtype name="Death">
      <soundset name="GenericMaleDeath">
      </soundset>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="MilitaryBirth">
      </soundset>
    </soundtype>
    <soundtype name="Acknowledge">
      <civlogic>
        <choice mergeMode="add" name="LBDanish">
          <targetlogic>
            <choice name="default">
              <soundset name="DanishHussarAcknowledge">
              </soundset>
            </choice>
            <choice name="enemy">
              <soundset name="DanishHussarAttack">
              </soundset>
            </choice>
          </targetlogic>
        </choice>
      </civlogic>
    </soundtype>
  </protounit>
</protounitsounddefmods>