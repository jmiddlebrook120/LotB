<protounitsounddef>
  <protounit name="Pikeman">
    <soundtype name="Select">
      <civlogic>
        <choice mergeMode="add" name="LBDanish">
          <soundset name="DanishPikemanSelect">
          </soundset>
        </choice>
      </civlogic>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="GenericMaleGrunt">
      </soundset>
    </soundtype>
    <soundtype name="Death">
      <soundset name="GenericMaleDeath">
      </soundset>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="MilitaryBirth">
      </soundset>
    </soundtype>
    <soundtype name="Acknowledge">
      <civlogic>
        <choice mergeMode="add" name="LBDanish">
          <targetlogic>
            <choice name="default">
              <soundset name="DanishPikemanAcknowledge">
              </soundset>
            </choice>
            <choice name="enemy">
              <soundset name="DanishPikemanAttack">
              </soundset>
            </choice>
          </targetlogic>
        </choice>
      </civlogic>
    </soundtype>
  </protounit>
</protounitsounddef>